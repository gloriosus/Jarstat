{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationDbContextConnection": "Host=localhost;Username=USERNAME;Password=PASSWORD;Database=DATABASE"
  },
  "TempStoragePath": "PATH_TO_FOLDER_FOR_FILE_UPLOADING",
  "JwtSecurityKey": "RND_GENERATED_STRING_OF_MINIMUM_16_CHARACTERS",
  "JwtTokenValidityMins": "NUMBER_OF_MINUTES"
}
